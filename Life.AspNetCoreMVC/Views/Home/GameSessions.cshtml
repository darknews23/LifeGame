@{
    ViewData["Title"] = "GameSessions";
}

@model GameSessionsViewModel

<div class="text-center">
    <p>GameSessions</p>

    <table>
        <thead>
        <tr>
            <th>Session Id</th>
            <th>Creation Date</th>
        </tr>
        </thead>
        <tbody>
            @foreach (var session in Model.SessionsInfo)
            {
            <tr>
                <td>@session.Id</td>
                <td>@session.Created</td>
            </tr>
            }
        </tbody>
    </table>
</div>
